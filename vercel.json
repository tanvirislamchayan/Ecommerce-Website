{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      }
    ],
    "env": {
      "VERCEL_ENV": "production",
      "DJANGO_SETTINGS_MODULE": "ecom.settings"
    }
  }
  